#include <stdio.h>
#define MAX_SIZE 100
int main()
 {
    int list[MAX_SIZE];
    int size, i;
    printf("Enter the size of the list: ");
    scanf("%d", &size);
    printf("Enter %d elements:\n", size);
    for (i = 0; i < size; i++)
    {
        scanf("%d", &list[i]);
    }
    printf("List: ");
    for (i = 0; i < size; i++) 
	{
        printf("%d ", list[i]);
    }
    printf("\n");
    return 0;
}
